  "scripts": {
    "start": "yarn install && yarn clean && vite",
    "dev": "yarn install && yarn clean && vite",
    "build-full": "sh -c 'NODE_ENV=ADNOFULL vite build --outDir adno-full --base ${PUBLIC_URL:-/}'",
    "build-light": "sh -c 'NODE_ENV=ADNOLIGHT vite build --outDir adno-light --base ${PUBLIC_URL:-/}'",
    "build": "yarn install && yarn clean && yarn build-full && yarn build-light",
    "clean": "rimraf dist && rimraf adno-full && rimraf adno-light",
    "clean-modules": "rm -r node_modules",
    "test": "npm run dev & npx playwright test ; npx playwright show-report"
  },
